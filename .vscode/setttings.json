{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "stylelint.validate": ["html", "astro"],
  "prettier.documentSelectors": [
		"**/*.astro",
		"**/*.json"
	],
	"prettier.formatOnSave": true,
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
